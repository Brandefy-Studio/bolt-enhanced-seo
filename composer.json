{
    "name": "brandefy-studio/bolt-enhanced-seo",
    "description": "Enhanced SEO extension for Bolt 5 and 6 with real-time analysis, meta tags, and Google preview",
    "type": "bolt-extension",
    "license": "MIT",
    "authors": [
        {
            "name": "Brandefy Studio",
            "email": "dev@brandefycreative.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "twig/twig": "^3.21"
    },
    "require-dev": {
        "bolt/core": "^5.0|^6.0",
        "phpstan/phpstan": "^2.1",
        "rector/rector": "^2.2",
        "symplify/easy-coding-standard": "^13",
        "phpstan/phpstan-symfony": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Brandefy\\BoltEnhancedSeo\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "entrypoint": "Brandefy\\BoltEnhancedSeo\\Extension"
    },
    "config": {
        "allow-plugins": {
            "symfony/runtime": true,
            "symfony/flex": true,
            "drupol/composer-packages": true
        }
    }
}
